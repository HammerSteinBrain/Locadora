<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Locadora</display-name>
  
   <servlet>
        <servlet-name>LoginCheck</servlet-name>
        <servlet-class>LoginCheck</servlet-class>
  </servlet>
  <servlet>
  		<servlet-name>Servlet_Cliente</servlet-name>
        <servlet-class>Servlet_Cliente</servlet-class>
  </servlet>
  <servlet>
  		<servlet-name>Servlet_ADM</servlet-name>
        <servlet-class>Servlet_ADM</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>LoginCheck</servlet-name>
    <url-pattern>/LoginCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_Cliente</servlet-name>
    <url-pattern>/Servlet_Cliente</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Servlet_ADM</servlet-name>
    <url-pattern>/Servlet_ADM</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>